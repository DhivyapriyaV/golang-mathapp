pipeline {
    agent {
        label 'docker-vm'
    }

    stages {
        
        stage(git-pull) {
            steps {
               checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/PriyaSIVATH/golang-mathapp.git']]) 
            }
        }



    }

}